<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">
    <RelativeLayout
        android:id="@+id/stuff_screen"
        android:layout_width="match_parent"
        android:background="@color/electric_green"
        android:layout_height="45dp">
        <TextView
            android:id="@+id/tv_model"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:textSize="18dp"
            android:text="@string/stuff_analy"
            android:textColor="@color/white"/>

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@mipmap/plus"
            android:onClick="toggleMenu"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="10dp"
            android:layout_marginStart="41dp" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.toc.dlpush.view.PieChart02View
            android:id="@+id/piechart"
            android:layout_width="match_parent"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_height="300dp" />
        <View
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:background="@drawable/circle"></View>
        </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <LinearLayout
            android:id="@+id/linearlayout01"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:orientation="horizontal"
            android:background="@color/electric_green"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="40dp"
            android:layout_alignParentStart="true"></LinearLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="装机率"
            android:textSize="10dp"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/linearlayout01"
            android:layout_toEndOf="@+id/linearlayout01"
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        >
        <LinearLayout
            android:id="@+id/linearlayout02"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:orientation="horizontal"
            android:background="@color/electric_green"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="40dp"
            android:layout_alignParentStart="true"></LinearLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="在线率"
            android:textSize="10dp"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/linearlayout02"
            android:layout_toEndOf="@+id/linearlayout02"
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        >
        <LinearLayout
            android:id="@+id/linearlayout03"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:orientation="horizontal"
            android:background="@color/electric_green"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="40dp"
            android:layout_alignParentStart="true"></LinearLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="未装机率"
            android:textSize="10dp"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/linearlayout03"
            android:layout_toEndOf="@+id/linearlayout03"
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        >
        <LinearLayout
            android:id="@+id/linearlayout04"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:orientation="horizontal"
            android:background="@color/electric_green"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="40dp"
            android:layout_alignParentStart="true"></LinearLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="未装机率"
            android:textSize="10dp"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/linearlayout04"
            android:layout_toEndOf="@+id/linearlayout04"
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp" />
    </RelativeLayout>
 <com.toc.dlpush.view.PieChart01View
     android:id="@+id/piechart01"
     android:layout_width="match_parent"
     android:layout_height="200dp" />
</LinearLayout>